@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
	ViewData["PageTitle"] = @Model.Value("pageTitle");
	ViewData["PageDescription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}

<header>
    @await Html.PartialAsync("_NavSection")
    @await Html.PartialAsync("_Showcase")
</header>

<main>
    @await Html.PartialAsync("_Brands")
    @await Html.PartialAsync("_Features")
    @await Html.PartialAsync("_AboutCompany")
    @await Html.PartialAsync("_OurServices")
    @await Html.PartialAsync("_WhyChooseUs")
    @await Html.PartialAsync("_ProjectsCaseStudies")
    @await Html.PartialAsync("Home Page/_MeetOurTeam")
    @await Html.PartialAsync("Home Page/_Testimonial")
    @await Html.PartialAsync("_ArticleNews")
</main>