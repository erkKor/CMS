@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;


<section class="our-services">
    <div class="container">
        <p>@Model.Value("ourServicesHeaderTitle")</p>
        <h2>@Model.Value("ourServicesDescription")</h2>
        <div class="_cards">
             @Html.GetBlockListHtml(Model.OurServicesBlockList)
        </div>
        <a href="@Model.Value("ourServicesLink")" class="button-light-theme">@Model.Value("ourServicesButtonText")<i class="fa-regular fa-arrow-up-right"></i></a>
    </div>
    <img src="~/static/background-lines-right.svg">
</section>